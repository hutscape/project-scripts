[secrets]
	allowed = Secret.h
	patterns = ssid\\[\\]\\s=\\s\".*\"
	allowed = ssid\\[\\]\\s=\\s\"secret\"
	patterns = pass\\[\\]\\s=\\s\".*\"
	allowed = pass\\[\\]\\s=\\s\"secret\"
	patterns = ssid\\s=\\s\".*\"
	allowed = ssid\\s=\\s\"secret\"
	patterns = password\\s=\\s\".*\"
	allowed = password\\s=\\s\"secret\"
	patterns = iftttKey\\s=\\s\".*\"
	allowed = iftttKey\\s=\\s\"secret\"
